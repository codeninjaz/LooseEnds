
@{
    ViewBag.Title = "Index";
}

@section Styles{
    @Styles.Render("~/Bundles/admincss")
}

@section Scripts {
<script type="text/javascript">
    var tokenKey = 'accessToken';

    $(function () {
        UpdateAdminInterface();
        window.setInterval(function () {
            UpdateAdminInterface();
        }, 10000)

    });

    function UpdateAdminInterface(){
        GetAdminData("/api/admin/usercount", "#userNumber");
        GetAdminData("/api/admin/gnuggiscount", "#gnuggisNumber");
        GetAdminData("/api/admin/AveragePointsCount", "#AveragePointsNumber");
        GetAdminData("/api/admin/CommentsCount", "#commentsNumber");
    }

    function GetAdminData(url, placeholder)
    {
        $.ajax({
            headers: { "Authorization": "bearer " + sessionStorage.getItem(tokenKey) },
            url: url,
            method: "GET",

        }).done(function (data) {
            $(placeholder).html(data);
        }).fail(function(data){
            console.log(data);
        });
    }
        
</script>

    @Scripts.Render("~/Bundles/adminscripts")

}

@section SideMenu {
    @Html.Partial("~/Views/Shared/_LeftMenu.cshtml")
}


<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            <i class="fa fa-dashboard"></i> Dashboard <small>Statistik överblick</small>
        </h1>
    </div>
</div>

<div class="row">
    <div class="col-lg-3 col-md-6">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class="fa fa-edit fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge" id="gnuggisNumber">0</div>
                        <div>Antal gnuggisar!</div>
                    </div>
                </div>
            </div>
            <a href="/admin/gnuggis">
